{
	"name": "natours",
	"version": "1.0.0",
	"description": "Landing page for natours",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass scss/main.scss css/style.css -w",
		"compile:sass": "node-sass scss/main.scss css/style.comp.css",
		"concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
		"prefix:css": "postcss --use autoprefixer -b 'last 5 versions' css/style.concant.css -o css/style.prefix.css",
		"compress:css": "node-sass css/style.concat.css css/style.css --output-style compressed",
		"build:css": ""
	},
	"author": "Lily Green",
	"license": "ISC",
	"devDependencies": {
		"concat": "^1.0.3",
		"node-sass": "^7.0.1",
		"postcss-cli": "^9.1.0"
	},
	"dependencies": {
		"autoprefixer": "^10.4.4"
	}
}
